{
  "name": "MicroMarkets",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "buildDebug": "cd android && ./gradlew assembleDebug",
    "buildRelease": "cd android && ./gradlew assembleRelease",
    "installDebug": "cd android && ./gradlew installDebug",
    "installRelease": "cd android && ./gradlew installRelease",
    "logs": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "openDeviceMenu": "adb shell input keyevent 82",
    "postinstall": "find . -name 'fbjs' -print | grep '\\./node_modules/fbjs' -v | xargs rm -rf",
    "start": "node node_modules/react-native/local-cli/cli.js start"
  },
  "dependencies": {
    "@remobile/react-native-splashscreen": "^1.0.3",
    "alt": "^0.18.1",
    "fbjs": "^0.8.1",
    "numeral": "^1.5.3",
    "query-string": "^4.0.2",
    "react": "^0.14.8",
    "react-native": "^0.23.1",
    "react-native-android-statusbar": "^0.1.2",
    "react-native-barcodescanner": "^1.0.1",
    "react-native-code-push": "^1.10.2-beta",
    "react-native-config": "0.0.3",
    "react-native-dropdown": "0.0.6",
    "react-native-lock": "^0.1.0",
    "react-native-lock-android": "0.0.3",
    "react-native-material-design": "git://github.com/saidimu/react-native-material-design.git",
    "react-native-vector-icons": "^1.3.4",
    "realm": "^0.11.1",
    "stripe": "^4.7.0"
  }
}
